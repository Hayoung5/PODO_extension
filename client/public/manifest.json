{
  "manifest_version": 3,
  "name": "PODO Wallet",
  "description": "PODO Wallet Extension",
  "version": "1.0.0",
  "icons": {
    "16": "podo_logo.png",
    "48": "podo_logo.png",
    "128": "podo_logo.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage"
  ],
  "action": {
    "default_icon": {
      "16": "podo_logo.png",
      "48": "podo_logo.png",
      "128": "podo_logo.png"
    },
    "default_popup": "index.html",
    "default_title": "PODO Wallet Extension"
  }
}